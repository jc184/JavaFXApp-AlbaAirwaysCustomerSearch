<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.cell.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="908.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapp.albaairwayscustomersearch.AlbaAirwaysCustomerSearchController">
    <children>
        <Button fx:id="buttonSearch" layoutX="417.0" layoutY="14.0" onAction="#handleSearchAction" text="Search" />
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
        <TextField fx:id="textFieldSearch" layoutX="480.0" layoutY="14.0" onAction="#handleSearchAction" />
        <TableView fx:id="tableView" layoutX="14.0" layoutY="52.0" prefHeight="337.0" prefWidth="876.0">
            <columns>
                <TableColumn prefWidth="75.0" text="CustomerId">
                    <cellValueFactory>   
                        <PropertyValueFactory property="CustomerId" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="100.0" text="First Name">
                    <cellValueFactory>   
                        <PropertyValueFactory property="FirstName" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="100.0" text="Last Name">
                    <cellValueFactory>   
                        <PropertyValueFactory property="Surname" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="100.0" text="Mobile No">
                    <cellValueFactory>   
                        <PropertyValueFactory property="MobileNo" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="100.0" text="Home Phone">
                    <cellValueFactory>   
                        <PropertyValueFactory property="HomePhoneNumber" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="100.0" text="Email">
                    <cellValueFactory>   
                        <PropertyValueFactory property="EmailAddress" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="100.0" text="Login Name">
                    <cellValueFactory>   
                        <PropertyValueFactory property="LoginName" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="100.0" text="Login Password">
                    <cellValueFactory>   
                        <PropertyValueFactory property="LoginPassword" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="100.0" text="Date of Birth">
                    <cellValueFactory>   
                        <PropertyValueFactory property="DateOfBirth" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
      <TextField fx:id="textFieldFind" layoutX="195.0" layoutY="14.0" onAction="#handleFindAction" />
      <Button fx:id="buttonFind" layoutX="135.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleFindAction" text="Find" />
    </children>
</AnchorPane>
